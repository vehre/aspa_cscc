<class name="window" standalone="true">
	<method name="open" type="window">
		<args />
  		<ast name="DOT">
  			<this />
			<ast name="METHOD_CALL" text="open">
				<ast name="ARGLIST_VALUES">
					<args />
				</ast>
			</ast>
		</ast>
	</method>
	<method name="close" type="window">
  		<ast name="DOT">
  			<this />
			<ast name="METHOD_CALL" text="close">
			</ast>
		</ast>
	</method>
	<method name="focus" type="VOID">
  		<ast name="DOT">
  			<this />
			<ast name="METHOD_CALL" text="focus" />
		</ast>
	</method>
	<method name="scrollBy" type="VOID">
		<arg type="DINT" />
		<arg type="DINT" />
  		<ast name="DOT">
  			<this />
			<ast name="METHOD_CALL" text="scrollBy">
				<ast name="ARGLIST_VALUES">
					<arg index="1" />
					<arg index="2" />
				</ast>
			</ast>
		</ast>
	</method>
	<method name="setTimeout" type="DINT">
		<args />
  		<ast name="DOT">
  			<this />
			<ast name="METHOD_CALL" text="setTimeout">
				<ast name="ARGLIST_VALUES">
					<args />
				</ast>
			</ast>
		</ast>
	</method>
	<method name="setInterval" type="DINT">
		<args />
  		<ast name="DOT">
  			<this />
			<ast name="METHOD_CALL" text="setInterval">
				<ast name="ARGLIST_VALUES">
					<args />
				</ast>
			</ast>
		</ast>
	</method>
</class>
