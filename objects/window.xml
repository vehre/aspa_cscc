<class name="window" standalone="true">
	<class name="MSDOMWindow">
	</class>
	<property name="status" type="DSTRING">
          <read>
		      <ast name="DOT">
				<this/>
				<ast name="IDENTIFIER" text="status" />
			  </ast>
          </read>
          <write>
		      <ast name="DOT">
				<this/>
				<ast name="IDENTIFIER" text="status" />
			  </ast>
          </write>
	</property>
	<method name="open" type="MSDOMWindow">
		<args />
  		<ast name="DOT">
  			<this />
			<ast name="METHOD_CALL" text="open">
				<ast name="ARGLIST_VALUES">
					<args />
				</ast>
			</ast>
		</ast>
	</method>
	<method name="close" type="VOID">
  		<ast name="DOT">
  			<this />
			<ast name="METHOD_CALL" text="close">
			</ast>
		</ast>
	</method>
	<method name="focus" type="VOID">
  		<ast name="DOT">
  			<this />
			<ast name="METHOD_CALL" text="focus" />
		</ast>
	</method>
	<method name="scrollBy" type="VOID">
		<arg type="DINT" />
		<arg type="DINT" />
  		<ast name="DOT">
  			<this />
			<ast name="METHOD_CALL" text="scrollBy">
				<ast name="ARGLIST_VALUES">
					<arg index="1" />
					<arg index="2" />
				</ast>
			</ast>
		</ast>
	</method>
	<method name="setTimeout" type="DINT">
		<args />
  		<ast name="DOT">
  			<this />
			<ast name="METHOD_CALL" text="setTimeout">
				<ast name="ARGLIST_VALUES">
					<args />
				</ast>
			</ast>
		</ast>
	</method>
	<method name="setInterval" type="DINT">
		<args />
  		<ast name="DOT">
  			<this />
			<ast name="METHOD_CALL" text="setInterval">
				<ast name="ARGLIST_VALUES">
					<args />
				</ast>
			</ast>
		</ast>
	</method>
</class>
