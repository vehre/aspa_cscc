<class name="document" dependant="true">
  <method name="all" type="VOID">
	<arg type="DSTRING" />
	<ast name="METHOD_CALL" text="document.getElementById">
		<ast name="ARGLIST_VALUES">
			<arg index="1" />
		</ast>
	</ast>
  </method>
  <method name="writeln" type="VOID">
    <arg type="DSTRING" />
    <ast name="METHOD_CALL" text="document.writeln">
        <ast name="ARGLIST_VALUES">
            <arg index="1" />
        </ast>
    </ast>
  </method>
  <method name="write" type="VOID">
    <arg type="DSTRING" />
    <ast name="METHOD_CALL" text="document.write">
        <ast name="ARGLIST_VALUES">
            <arg index="1" />
        </ast>
    </ast>
  </method>
</class>
