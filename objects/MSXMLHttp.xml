<class name="MSXMLHttp" standalone="true">
	<constructor>
		<ast name="NEW">
        	<ast name="IDENTIFIER" text="ActiveXObject" />
       			<ast name="ARGLIST_VALUES">
       				<ast name="DSTRING" text="Microsoft.xmlhttp" />
       			</ast>
        </ast>
	</constructor>
	
	
	<!--
	<property name="responseXML" type="DSTRING">
		<read>
			 <ast name="DOT">
                <this />
                <ast name="IDENTIFIER" text="responseXML" />
            </ast>
		</read>	
	</property>
	-->
	<property name="responseXML" type="MSXMLDOM">
		<read>
			 <ast name="DOT">
                <this />
                <ast name="IDENTIFIER" text="responseXML" />
            </ast>
		</read>	
	</property>
	
	<method name="open" type="VOID">
		<arg type="DSTRING"/>
		<arg type="DSTRING"/>
		<arg type="DBOOL"/>
		<ast name="DOT">
			<this />
			<ast name="METHOD_CALL" text="open">
				<ast name="ARGLIST_VALUES">
					<arg index="1" />
					<arg index="2" />
					<arg index="3" />
				</ast>
			</ast>
		</ast>
	</method>
	
	<method name="send" type="VOID">
		<args />
		<ast name="DOT">
			<this />
			<ast name="METHOD_CALL" text="send">
				<ast name="ARGLIST_VALUES">
					<args />
				</ast>
			</ast>
		</ast>
	</method>
</class>

