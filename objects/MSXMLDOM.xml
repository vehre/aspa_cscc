<class name="MSXMLDOM" standalone="true">
	<property name="documentElement" type="MSDOMDocument">
		<read>
			 <ast name="DOT">
                <this />
                <ast name="IDENTIFIER" text="documentElement" />
            </ast>
		</read>
	</property>
	<property name="xml" type="DSTRING">
		<read>
			 <ast name="DOT">
                <this />
                <ast name="IDENTIFIER" text="xml" />
            </ast>
		</read>
	</property>
	
	<constructor>
		<ast name="NEW">
        	<ast name="IDENTIFIER" text="ActiveXObject"/>
       			<ast name="ARGLIST_VALUES">
       				<ast name="DSTRING" text="Microsoft.xmldom"/>
       			</ast>
        </ast>
	</constructor>
  
  	<method name="loadXML" type="VOID">
		<arg type="DSTRING"/>
		<ast name="DOT">
			<this />
			<ast name="METHOD_CALL" text="loadXML">
				<ast name="ARGLIST_VALUES">
					<arg index="1" />
				</ast>
			</ast>
		</ast>
	</method>
	
	<method name="selectSingleNode" type="MSXMLDOM">
		<arg type="DSTRING"/>
		<ast name="DOT">
			<this/>
			<ast name="METHOD_CALL" text="selectSingleNode">
			  <ast name="ARGLIST_VALUES">
				<arg index="1" />
			  </ast>
			</ast>
		</ast>
	</method>
	
	<method name="getAttribute" type="DSTRING">
		<arg type="DSTRING"/>
		<ast name="DOT">
			<this/>
			<ast name="METHOD_CALL" text="getAttribute">
			  <ast name="ARGLIST_VALUES">
				<arg index="1" />
			  </ast>
			</ast>
		</ast>
	</method>
</class>

	