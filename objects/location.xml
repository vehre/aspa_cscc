<class name="location">
	<property name="search" type="DSTRING">
		<read>
			 <ast name="DOT">
				<this />
				<ast name="IDENTIFIER" text="search" />
			</ast>
		</read>
		<write>
			<ast name="ASSIGN">
				<ast name="DOT">
					<this />
					<ast name="IDENTIFIER" text="search" />
				</ast>
				<arg index="1" />
			</ast>
		</write>
	</property>
	<property name="href" type="DSTRING">
		<read>
			<ast name="DOT">
				<this/>
				<ast name="IDENTIFIER" text="href" />
			</ast>
		</read>
		<write>
			<ast name="ASSIGN">
				<ast name="DOT">
					<this/>
					<ast name="IDENTIFIER" text="href" />
				</ast>
				<arg index="1" />
			</ast>
		</write>
	</property>
</class>

