    </method>

    <method name="tan" type="DINT">
        <arg type="DINT" />
        <ast name="METHOD_CALL" text="tan">
            <ast name="ARGLIST_VALUES">
                <arg index="1" />
            </ast>
        </ast>
    </method>
</class>
