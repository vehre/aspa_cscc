                <arg index="1" />
            </ast>
        </ast>
    </method>

    <method name="max" type="DINT">
        <args />
        <ast name="METHOD_CALL" text="max">
            <ast name="ARGLIST_VALUES">
                <args />
            </ast>
        </ast>
    </method>

    <method name="min" type="DINT">
        <args />
        <ast name="METHOD_CALL" text="min">
            <ast name="ARGLIST_VALUES">
                <args />
            </ast>
        </ast>
    </method>

    <method name="pow" type="DINT">
        <arg type="DINT" />
        <arg type="DINT" />
        <ast name="METHOD_CALL" text="pow">
            <ast name="ARGLIST_VALUES">
                <arg index="1" />
                <arg index="2" />
            </ast>
        </ast>
    </method>

    <method name="random" type="DINT">
        <ast name="METHOD_CALL" text="rand" />
    </method>

    <method name="round" type="DINT">
