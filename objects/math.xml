                <arg index="1" />
            </ast>
        </ast>
    </method>

    <method name="exp" type="DINT">
        <arg type="DINT" />
        <ast name="METHOD_CALL" text="exp">
            <ast name="ARGLIST_VALUES">
                <arg index="1" />
            </ast>
        </ast>
    </method>

    <method name="floor" type="DINT">
        <arg type="DINT" />
        <ast name="METHOD_CALL" text="floor">
            <ast name="ARGLIST_VALUES">
                <arg index="1" />
            </ast>
        </ast>
