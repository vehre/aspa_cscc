<class name="Math" dependant="true">
    <property name="E" type="DFLOAT">
        <read>
            <ast name="CONSTANT" text="M_E"/>
        </read>
    </property>

    <property name="LN2" type="DFLOAT">
        <read>
            <ast name="CONSTANT" text="M_LN2"/>
        </read>
    </property>

    <property name="LN10" type="DFLOAT">
        <read>
            <ast name="CONSTANT" text="M_LN10"/>
        </read>
    </property>

    <property name="LOG2E" type="DFLOAT">
        <read>
            <ast name="CONSTANT" text="M_LOG2E"/>
        </read>
    </property>

    <property name="LOG10E" type="DFLOAT">
        <read>
            <ast name="CONSTANT" text="M_LOG10E"/>
        </read>
    </property>

    <property name="PI" type="DFLOAT">
        <read>
            <ast name="CONSTANT" text="M_PI"/>
        </read>
    </property>

    <property name="SQRT1_2" type="DFLOAT">
        <read>
            <ast name="CONSTANT" text="M_SQRT1_2"/>
        </read>
    </property>

    <property name="SQRT2" type="DFLOAT">
        <read>
            <ast name="CONSTANT" text="M_SQRT2"/>
        </read>
    </property>

    <method name="abs" type="DINT">
        <arg type="DINT" />
        <ast name="METHOD_CALL" text="abs">
            <ast name="ARGLIST_VALUES">
                <arg index="1" />
            </ast>
        </ast>
    </method>

    <method name="acos" type="DINT">
        <arg type="DINT" />
        <ast name="METHOD_CALL" text="acos">
            <ast name="ARGLIST_VALUES">
                <arg index="1" />
            </ast>
        </ast>
    </method>

    <method name="asin" type="DINT">
        <arg type="DINT" />
        <ast name="METHOD_CALL" text="asin">
            <ast name="ARGLIST_VALUES"> 
                <arg index="1" />
            </ast>
        </ast>
    </method>

    <method name="atan" type="DINT">
        <arg type="DINT" />
        <ast name="METHOD_CALL" text="atan">
            <ast name="ARGLIST_VALUES">
                <arg index="1" />
            </ast>
        </ast>
    </method>

    <method name="atan2" type="DINT">
