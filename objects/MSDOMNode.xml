<class name="MSDOMNode" dependant="true">
	<property name="bgcolor" type="DSTRING">
		<read>
			 <ast name="DOT">
				<this />
				<ast name="IDENTIFIER" text="bgcolor" />
			</ast>
		</read>
		<write>
			<ast name="ASSIGN">
				<ast name="DOT">
					<this />
					<ast name="IDENTIFIER" text="bgcolor" />
				</ast>
				<arg index="1" />
			</ast>
		</write>
	</property>
	<property name="text" type="DSTRING">
		<read>
			 <ast name="DOT">
				<this />
				<ast name="IDENTIFIER" text="text" />
			</ast>
		</read>
		<write>
			<ast name="ASSIGN">
				<ast name="DOT">
					<this />
					<ast name="IDENTIFIER" text="text" />
				</ast>
				<arg index="1" />
			</ast>
		</write>
	</property>
	<property name="Value" type="DSTRING">
		<read>
			 <ast name="DOT">
				<this />
				<ast name="IDENTIFIER" text="value" />
			</ast>
		</read>
	</property>
	<property name="ReadOnly" type="DBOOL">
		<read>
			 <ast name="DOT">
				<this />
				<ast name="IDENTIFIER" text="readonly" />
			</ast>
		</read>
		<write>
			<ast name="ASSIGN">
				<ast name="DOT">
					<this />
					<ast name="IDENTIFIER" text="readonly" />
				</ast>
				<arg index="1" />
			</ast>
		</write>
	</property>
	
	<method name="Focus" type="VOID">
		<ast name="DOT">
			<this />
			<ast name="METHOD_CALL" text="focus" />
		</ast>
	</method>
	<method name="Select" type="VOID">
		<ast name="DOT">
			<this/>
			<ast name="METHOD_CALL" text="select">
			</ast>
		</ast>
	</method>
	
	<method name="getAttribute" type="DSTRING">
		<arg type="DSTRING"/>
		<ast name="DOT">
			<this/>
			<ast name="METHOD_CALL" text="getAttribute">
			  <ast name="ARGLIST_VALUES">
				<arg index="1" />
			  </ast>
			</ast>
		</ast>
	</method>
	
	<!-- Flash Element functions -->
	<property name="Movie" type="DSTRING">
		<read>
			 <ast name="DOT">
				<this />
				<ast name="IDENTIFIER" text="Movie" />
			</ast>
		</read>
	</property>
	<method name="loadmovie" type="DSTRING">
		<arg type="DINT"/>
		<arg type="DSTRING"/>
		<ast name="DOT">
			<this/>
			<ast name="METHOD_CALL" text="LoadMovie">
				<ast name="ARGLIST_VALUES">
					<arg index="1" />
					<arg index="2" />
				</ast>
			</ast>
		</ast>
	</method>
	<method name="Play" type="VOID">
		<ast name="DOT">
			<this/>
			<ast name="METHOD_CALL" text="Play">
			</ast>
		</ast>
	</method>
	<method name="Rewind" type="VOID">
		<ast name="DOT">
			<this/>
			<ast name="METHOD_CALL" text="Rewind">
			</ast>
		</ast>
	</method>
	<method name="StopPlay" type="VOID">
		<ast name="DOT">
			<this/>
			<ast name="METHOD_CALL" text="StopPlay">
			</ast>
		</ast>
	</method>
</class>
